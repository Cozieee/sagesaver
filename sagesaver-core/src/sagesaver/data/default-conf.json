{
    "sagesaver": {
        "log_dir": "/var/log/sagesaver",
        "tmp_dir": "/var/tmp/sagesaver"
    },
    "interface": {
        "type": "user"
    },
    "server": {
        "type": null,
        "env_stack_name": null,
        "idle_time_limit": 30,
        "log_time_format": "%Y-%m-%d %H:%M:%S"
    },
    "database":{
        "type": null
    },
    "jupyter": {
        "config_time_format": "%Y-%m-%d %H:%M:%S",
        "log_path": "/var/log/sagesaver/jupyter.log"
    },
    "mongo": {
        "dump_path": "mongo.dump"
    },
    "mysql": {
        "log_path": "mysql.log"
    }
}